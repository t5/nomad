{
  "manifest_version": 2,
  "name": "Nomad",
  "version": "0.1",

  "description": "Create bookmarks that can be updated.",
  "browser_action": {
    "default_popup": "popup.html"
  },
  "permissions": ["storage", "tabs"],
  "background": {
    "persistent": true,
    "scripts": ["background.js"]
  }
}
